{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "electron": "electron .",
    "hot": ".\\node_modules\\.bin\\gulp watch:electron",
    "build:dir": "electron-builder --dir",
    "build:dist": "electron-builder",
    "start": "webpack --config ./app/webpack.config.js",
    "dev": "webpack-dev-server --config ./app/webpack.config.js",
    "build:txt": "webpack --progress --hide-modules --config ./app/webpack.config.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^22.4.1",
    "mysql": "^2.18.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "iconv-lite": "^0.5.1",
    "mysql": "^2.18.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "build": {
    "files": [
      "**/*",
      "!app/*",
      "app/dist/*",
      "app/index.html",
      "app/dist/node_modules/*",
      "!dist/*"
    ]
  }
}
